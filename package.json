{
    "author": {
        "email": "khushrajrathod@gmail.com",
        "name": "Khushraj Rathod"
    },
    "build": {
        "appId": "teledrive",
        "mac": {
            "category": "uploader"
        },
        "directories": {
            "buildResources": "icon"
        },
        "extraResources": [
            {
                "from": "tdlib/${os}",
                "to": "tdlib/${os}",
                "filter": [
                    "**/*"
                ]
            }
        ],
        "files": [
            "src/",
            "LICENSE",
            "icon/tray.png",
            "icon/trayTemplate.png",
            "icon/tray.ico"
        ],
        "snap": {
            "publish": ["github"]
        }
    },
    "dependencies": {
        "airgram": "latest",
        "chokidar": "latest",
        "electron-store": "latest",
        "ncp": "latest",
        "electron-updater": "^4.3.4"
    },
    "description": "An app that lets you automatically backup to Telegram Saved Messages",
    "devDependencies": {
        "electron": "latest",
        "electron-builder": "^22.7.0"
    },
    "homepage": "https://khushrajrathod.github.io/TeleDrive",
    "keywords": [
        "TeleDrive",
        "telegram",
        "telegram client",
        "tdlib"
    ],
    "license": "UNLICENSED",
    "main": "src/app.js",
    "name": "teledrive",
    "productName": "TeleDrive",
    "repository": "https://github.com/KhushrajRathod/TeleDrive",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "start": "electron ."
    },
    "version": "0.8.2"
}
