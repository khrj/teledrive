{
    "author": {
        "email": "khushrajrathod@gmail.com",
        "name": "Khushraj Rathod"
    },
    "build": {
        "appId": "teledrive",
        "mac": {
            "category": "uploader"
        },
        "extraResources": [
            {
                "from": "tdlib/${os}",
                "to": "bin",
                "filter": [
                    "**/*"
                ]
            }
        ]
    },
    "dependencies": {
        "airgram": "latest",
        "chokidar": "latest",
        "electron-builder-squirrel-windows": "^22.8.0",
        "electron-squirrel-startup": "latest",
        "electron-store": "latest",
        "ncp": "latest"
    },
    "description": "An app that lets you automatically backup to Telegram Saved Messages",
    "devDependencies": {
        "electron": "latest",
        "electron-builder": "^22.7.0"
    },
    "homepage": "https://khushrajrathod.github.io/TeleDrive",
    "keywords": [
        "TeleDrive",
        "telegram",
        "telegram client",
        "tdlib"
    ],
    "license": "UNLICENSED",
    "main": "src/app.js",
    "name": "teledrive",
    "productName": "TeleDrive",
    "repository": "https://github.com/KhushrajRathod/TeleDrive",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "start": "electron ."
    },
    "version": "0.6.0"
}
